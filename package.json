{
  "name": "todo",
  "version": "0.1.0",
  "homepage": "https://andrey4100.github.io/todo.github.io",
  "private": true,
  "dependencies": {
    "cra-template": "1.2.0",
    "date-fns": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx}",
    "deploy": "gh-pages -d build",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
